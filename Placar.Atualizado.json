              "raw": "{\n  \"devices\": [\n    {\n      \"device_id\": \"score001\",\n      \"entity_name\": \"urn:ngsi-ld:Score:001\",\n      \"entity_type\": \"Score\",\n      \"protocol\": \"PDI-IoTA-UltraLight\",\n      \"transport\": \"MQTT\",\n      \"commands\": [\n        {\n          \"name\": \"ga_add\",\n          \"type\": \"command\"\n        },\n        {\n          \"name\": \"ga_sub\",\n          \"type\": \"command\"\n        },\n        {\n          \"name\": \"gb_add\",\n          \"type\": \"command\"\n        },\n        {\n          \"name\": \"gb_sub\",\n          \"type\": \"command\"\n        },\n        {\n          \"name\": \"y_add\",\n          \"type\": \"command\"\n        },\n        {\n          \"name\": \"y_sub\",\n          \"type\": \"command\"\n        },\n        {\n          \"name\": \"r_add\",\n          \"type\": \"command\"\n        },\n        {\n          \"name\": \"r_sub\",\n          \"type\": \"command\"\n        },\n        {\n          \"name\": \"timer_toggle\",\n          \"type\": \"command\"\n        },\n        {\n          \"name\": \"timer_reset\",\n          \"type\": \"command\"\n        }\n      ],\n      \"attributes\": [\n        {\n          \"object_id\": \"gA\",\n          \"name\": \"gA\",\n          \"type\": \"Integer\"\n        },\n        {\n          \"object_id\": \"gB\",\n          \"name\": \"gB\",\n          \"type\": \"Integer\"\n        },\n        {\n          \"object_id\": \"y\",\n          \"name\": \"y\",\n          \"type\": \"Integer\"\n        },\n        {\n          \"object_id\": \"r\",\n          \"name\": \"r\",\n          \"type\": \"Integer\"\n        },\n        {\n          \"object_id\": \"run\",\n          \"name\": \"run\",\n          \"type\": \"Integer\"\n        },\n        {\n          \"object_id\": \"t\",\n          \"name\": \"t\",\n          \"type\": \"Text\"\n        },\n        {\n          \"object_id\": \"online\",\n          \"name\": \"online\",\n          \"type\": \"Integer\"\n        }\n      ]\n    }\n  ]\n}"
            },
